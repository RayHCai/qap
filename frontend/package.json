{
	"name": "qaapp-frontend",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "webpack-dev-server --mode=development --open --hot",
		"build": "webpack --mode=production",
		"lint": "eslint \"**/*.{ts,tsx}\"",
		"lint:fix": "eslint --fix --ext .ts,.tsx ."
	},
	"dependencies": {
		"husky": "^8.0.3",
		"lint-staged": "^13.2.3",
		"prettier": "^3.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.10.1",
		"react-router": "^6.14.2",
		"react-router-dom": "^6.14.2"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@babel/preset-env": "^7.22.9",
		"@babel/preset-react": "^7.22.5",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.10",
		"@types/react-icons": "^3.0.0",
		"@types/react-router": "^5.1.20",
		"@types/react-router-dom": "^5.3.3",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"babel-loader": "^9.1.3",
		"css-loader": "^6.8.1",
		"eslint": "^8.44.0",
		"eslint-plugin-react": "^7.32.2",
		"html-webpack-plugin": "^5.5.3",
		"style-loader": "^3.3.3",
		"ts-loader": "^9.4.4",
		"typescript": "^4.9.5",
		"webpack": "^5.88.1",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not op_mini all"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"prettier --write",
			"yarn lint --fix",
			"git add"
		],
		"*.{html,css}": [
			"prettier --write",
			"git add"
		]
	}
}
